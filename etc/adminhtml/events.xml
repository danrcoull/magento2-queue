<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="catalog_product_save_after">
        <observer name="springbot_queue_catalog_product_save_after" instance="Springbot\Queue\Observer\ProductSaveObserver"/>
    </event>
    <event name="catalog_product_delete_after">
        <observer name="springbot_queue_catalog_product_delete_after" instance="Springbot\Queue\Observer\ProductDeleteObserver"/>
    </event>
    <event name="catalog_category_save_after">
        <observer name="springbot_queue_catalog_category_save_after" instance="Springbot\Queue\Observer\CategorySaveObserver"/>
    </event>
    <event name="catalog_category_delete_after">
        <observer name="springbot_queue_catalog_category_delete_after" instance="Springbot\Queue\Observer\CategoryDeleteObserver"/>
    </event>
    <event name="customer_save_after">
        <observer name="springbot_queue_customer_save_after" instance="Springbot\Queue\Observer\CustomerSaveObserver"/>
    </event>
    <event name="customer_delete_after">
        <observer name="springbot_queue_customer_delete_after" instance="Springbot\Queue\Observer\CustomerDeleteObserver"/>
    </event>
    <event name="newsletter_subscriber_save_after">
        <observer name="springbot_queue_newsletter_subscriber_save_after" instance="Springbot\Queue\Observer\SubscriberSaveObserver"/>
    </event>
    <event name="eav_entity_attribute_save_commit_after">
        <observer name="springbot_queue_eav_entity_attribute_save_commit_after" instance="Springbot\Queue\Observer\AttributeSaveObserver"/>
    </event>
</config>
