<?xml version="1.0"?>

<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/springbot/queue/config" method="GET">
        <service class="Springbot\Queue\Api\ConfigInterface" method="getConfig"/>
        <resources>
            <resource ref="Springbot_Queue::queue"/>
        </resources>
    </route>
</routes>
