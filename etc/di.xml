<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="queue_process" xsi:type="object">Springbot\Queue\Console\Command\ProcessQueue</item>
                <item name="queue_enqueue" xsi:type="object">Springbot\Queue\Console\Command\Enqueue</item>
            </argument>
        </arguments>
    </type>

    <preference for="Springbot\Queue\Api\ConfigInterface" type="Springbot\Queue\Helper\Data"/>
</config>
